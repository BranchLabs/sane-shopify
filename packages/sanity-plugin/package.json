{
	"name": "@sane-shopify/sanity-plugin",
	"version": "0.2.1",
	"description": "A Sanity.io field for selecting products & collections from a Shopify storefront",
	"author": "Joseph Thomas <joseph@good-idea.studio>",
	"homepage": "https://github.com/good-idea/sane-shopify#readme",
	"license": "ISC",
	"keywords": [
		"sanity",
		"shopify"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/good-idea/sane-shopify.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"directories": {
		"lib": "lib",
		"test": "__tests__"
	},
	"files": [
		"lib",
		"sanity.json"
	],
	"main": "lib/index.js",
	"module": "lib/index.es.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"fix": "run-s fix:*",
		"fix:prettier": "prettier \"src/**/*.ts\" --write --loglevel warn",
		"fix:tslint": "tslint --fix --project .",
		"test": "run-s build test:lint test:unit",
		"test:lint": "tslint --project . && prettier \"src/**/*.ts\" --list-different",
		"test:unit": "jest --collectCoverage",
		"test:watch": "jest --watch",
		"test:coverage:open": "yarn test:unit && opn coverage/lcov-report/index.html",
		"test:coverage:send": "codecov",
		"build": "rollup -c",
		"watch": "rollup -cw",
		"prepare": "npm run build"
	},
	"bugs": {
		"url": "https://github.com/good-idea/sane-shopify/issues"
	},
	"peerDependencies": {
		"react": "16.2",
		"react-dom": "16.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.5",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.4.4",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-typescript": "^7.3.3",
		"@sanity/base": "^0.140.17",
		"@sanity/components": "^0.140.25",
		"@sanity/form-builder": "^0.140.25",
		"@types/babel__core": "^7.1.2",
		"@types/lodash": "^4.14.132",
		"@types/lodash-es": "^4.17.3",
		"@types/ramda": "^0.26.8",
		"@types/react": "16.8.18",
		"@types/react-dom": "16.8.4",
		"@types/rimraf": "^2.0.2",
		"babel-polyfill": "^6.26.0",
		"npm-run-all": "^4.1.5",
		"react": "16.2",
		"react-dom": "16.2",
		"react-hot-loader": "^4.8.8",
		"rimraf": "^2.6.3",
		"rollup": "^1.15.6",
		"tslint": "^5.16.0"
	},
	"dependencies": {
		"@good-idea/unwind-edges": "^1.0.2",
		"@sane-shopify/sync-utils": "^0.2.0",
		"@sane-shopify/types": "^0.2.0",
		"lodash": "^4.17.11",
		"lodash-es": "^4.17.11",
		"ramda": "^0.26.1",
		"react-icons": "^3.7.0",
		"rxjs": "^6.5.2"
	},
	"gitHead": "26ed458ab898470e7fcd2cd9050f5759546d7623"
}
